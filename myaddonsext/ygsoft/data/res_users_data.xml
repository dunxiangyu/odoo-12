            <field name="company_ids" eval="[(6, 0, [ref('yd_ygsoft_company')])]"/>
            <field name="login">wlj</field>
            <field name="password">wlj</field>
            <field name="groups_id" eval="[(6,0,[ref('base.group_user')])]"/>
